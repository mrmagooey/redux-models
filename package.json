{
  "name": "redux-simple-models",
  "version": "0.1.1",
  "description": "getters and setters for redux models",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_ENV=test node_modules/.bin/mocha --compilers js:babel-core/register tests",
    "test-watch": "NODE_ENV=test node_modules/.bin/mocha --compilers js:babel-core/register --watch tests",
    "test-coverage": "NODE_ENV=test nyc npm test",
    "build-js": "browserify src/index.js -o build/index.js -t [ babelify --presets [ es2015 ] ]",
    "minify": "uglifyjs --compress --mangle --screw-ie8 --output build/index.js -- build/index.js",
    "build": "npm run-script build-js && npm run-script minify"
  },
  "keywords": [
    "redux"
  ],
  "author": "mrmagooey",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.13.0",
    "babelify": "^7.3.0",
    "browserify-shim": "^3.8.12",
    "mocha": "^3.0.2",
    "nyc": "^8.1.0",
    "redux": "^3.6.0",
    "uglify": "^0.1.5"
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "lodash": "^4.15.0"
  },
  "browserify": {
    "transform": [["babelify", {"presets": ["es2015"]}]]
  }
}
